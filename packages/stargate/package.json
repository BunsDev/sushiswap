{
  "name": "soulswap-stargate",
  "version": "0.1.0",
  "description": "Soul Stargate",
  "keywords": [
    "soul",
    "stargate"
  ],
  "license": "MIT",
  "author": "0xBuns",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "source": "src/index.ts",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/**",
    "abis/**",
    "IStargateReceiver.sol",
    "IStargateRouter.sol"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
    "lint": "TIMING=1 eslint src --fix",
    "prepublishOnly": "pnpm build",
    "test": "jest --passWithNoTests"
  },
  "jest": {
    "preset": "soulswap-jest-config/node"
  },
  "devDependencies": {
    "@sushiswap/chain": "workspace:*",
    "@sushiswap/currency": "workspace:*",
    "soulswap-eslint-config": "workspace:*",
    "soulswap-jest-config": "workspace:*",
    "@sushiswap/math": "workspace:*",
    "@sushiswap/prettier-config": "workspace:*",
    "soulswap-typescript-config": "workspace:*",
    "@types/jest": "^27.5.1",
    "eslint": "^8.20.0",
    "jest": "^28.1.0",
    "tiny-invariant": "^1.2.0",
    "tsup": "^6.2.0",
    "typescript": "4.8.2"
  }
}