{
  "name": "soulswap-chain",
  "version": "0.1.0",
  "description": "Soul Chain",
  "keywords": [
    "soul",
    "chain"
  ],
  "license": "MIT",
  "author": "0xBuns",
  "main": "./dist/index.js",
  "source": "./index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsc -w",
    "generate": "ls chains.json || curl 'https://chainid.network/chains.json' > chains.json",
    "preinstall": "yarn run generate",
    "lint": "TIMING=1 eslint ./index.ts --fix",
    "test": "jest --passWithNoTests"
  },
  "jest": {
    "preset": "soulswap-jest-config/node"
  },
  "devDependencies": {
    "soulswap-eslint-config": "workspace:*",
    "soulswap-jest-config": "workspace:*",
    "soulswap-prettier-config": "workspace:*",
    "soulswap-typescript-config": "workspace:*",
    "@types/jest": "^27.5.1",
    "eslint": "^8.20.0",
    "jest": "^28.1.0",
    "typescript": "4.8.2"
  }
}