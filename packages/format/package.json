{
  "name": "soulswap-format",
  "version": "0.0.1",
  "description": "Soul Format",
  "keywords": [
    "soul",
    "format"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/soulswapfinance/soulswap.git",
    "directory": "packages/format"
  },
  "license": "MIT",
  "author": "0xBuns",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "source": "src/index.ts",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
    "lint": "TIMING=1 eslint src --fix",
    "prepublishOnly": "yarn build",
    "test": "jest --passWithNoTests"
  },
  "jest": {
    "preset": "soulswap-jest-config/node"
  },
  "dependencies": {
    "@ethersproject/address": "^5.7.0",
    "millify": "^4.0.0",
    "numeral": "^2.0.6"
  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/numeral": "^2.0.2",
    "eslint": "^8.20.0",
    "jest": "^28.1.0",
    "soulswap-eslint-config": "workspace:*",
    "soulswap-jest-config": "workspace:*",
    "soulswap-prettier-config": "workspace:*",
    "soulswap-typescript-config": "workspace:*",
    "tsup": "^6.2.0",
    "typescript": "4.8.2"
  }
}
